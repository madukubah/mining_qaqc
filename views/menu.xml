<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record model="ir.actions.act_window" id="action_coa">
        <field name="name">COA</field>
        <field name="res_model">qaqc.coa</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record model="ir.actions.act_window" id="action_surveyor">
        <field name="name">Surveyor</field>
        <field name="res_model">res.partner</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{"search_default_is_surveyor":1}</field>
    </record>

    
    <menuitem id="qaqc_top" name="QAQC" sequence="1" web_icon="sale_qaqc,static/description/icon.png" />

    <menuitem id="qaqc_sub" name="QAQC" sequence="1" 
			parent="qaqc_top"/>
    
        <menuitem name="COA"
                id="qaqc_coa"
                parent="qaqc_sub" 
                action="action_coa"
                sequence= "1"
                />
    <menuitem id="qaqc_surveyor" 
            name="Surveyor" 
            sequence="1" 
			parent="qaqc_top"
            action="action_surveyor"
            />

</odoo>
